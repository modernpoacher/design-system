{
  "name": "@modernpoacher/design-system",
  "version": "0.0.639",
  "description": "Modern Poacher Design System",
  "keywords": [
    "Modern",
    "Poacher",
    "Design",
    "System"
  ],
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": "https://github.com/modernpoacher/design-system.git",
  "scripts": {
    "build": "npm run gulp",
    "start": "cross-env NODE_ENV=production npm run build",
    "dev": "cross-env NODE_ENV=development npm run build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "gulp": "gulp --color",
    "pre-commit": "npm run lint",
    "post-commit": "cross-env DEBUG=@modernpoacher/hooks,@modernpoacher/hooks:* npm run gulp -- post-commit"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@modernpoacher/hooks": "^1.0.163",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0"
  }
}
